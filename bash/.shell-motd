# Console prints may be skipped
if [ "$1" != "silent" ]; then

	echo
	figlet -c $SHORTHOSTNAME 2>/dev/null || echo "$SHOTHOSTNAME"
	echo

	# Print status of important folders
	git-stat-folder() { _hasFolder $1 && printf " %-9s @ %s\n" "$1" "$(git-head-stat $1)"; }
  git-stat-folder .dotfiles
	echo

fi
