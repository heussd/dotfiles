{
  "disableAnalytics": true,
  "hotkey": "alt+space",
  "theme": "heussd/zazu-simple-and-dense-theme",
  "hideTrayItem": "True",
  "displayOn": "detect",
  "plugins": [
    {
      "name": "tinytacoteam/zazu-fallback",
      "variables": {
        "rootSearches": [
          "g", "dockerhub", "ap"
        ],
        "prefixSearches": {
          "g": {
            "icon": "fa-search",
            "name": "Google",
            "url": "https://www.google.de/search?q="
          },
          "ap": {
            "icon": "fa-cube",
            "name": "Alpine Package Search",
            "url": "https://pkgs.alpinelinux.org/packages?name="
          },
          "af": {
            "icon": "fa-cube",
            "name": "Alpine File Search",
            "url": "https://pkgs.alpinelinux.org/contents?file="
          },
          "cs": {
            "icon": "fa-search",
            "name": "Google Cloud Search",
            "url": "https://cloudsearch.google.com/cloudsearch/search?q="
          },
          "css": {
            "icon": "fa-css3",
            "name": "cssreference.io",
            "url": "https://cssreference.io/property/"
          },
          "yt": {
            "icon": "fa-youtube",
            "name": "Youtube",
            "url": "https://www.youtube.com/results?search_query="
          },
          "mail": {
            "icon": "fa-envelope-open",
            "name": "Google Mail",
            "url": "https://mail.google.com/mail/u/0/#search/"
          },
          "mvn": {
            "icon": "fa-book",
            "name": "Maven Central",
            "url": "http://mvnrepository.com/search?q="
          },
          "gif": {
            "icon": "fa-smile",
            "name": "Giphy",
            "url": "https://giphy.com/search/"
          },
          "doi": {
            "icon": "fa-library",
            "name": "DOI",
            "url": "http://dx.doi.org/"
          },
          "speak": {
            "icon": "fa-headphones",
            "name": "Forvo",
            "url": "https://www.forvo.com/search/"
          },
          "fa": {
            "icon": "fa-book",
            "name": "Font Awesome",
            "url": "https://fontawesome.com/icons?q="
          },
          "d": {
            "icon": "fa-globe",
            "name": "dict.cc",
            "url": "http://www.dict.cc/?s="
          },
          "ling": {
            "icon": "fa-globe",
            "name": "Linguee",
            "url": "https://www.linguee.de/deutsch-englisch/search?source=auto&query="
          },
          "dockerhub": {
            "icon": "fa-cloud",
            "name": "Docker Hub",
            "url": "https://hub.docker.com/search/?q="
          },
          "alt": {
            "icon": "fa-box",
            "name": "AlternativeTo.net",
            "url": "https://alternativeto.net/browse/search?q="
          },
          "stats": {
            "icon": "fa-signal",
            "name": "Statista",
            "url": "https://www.statista.com/search/?q="
          },
          "deepl": {
            "name": "DeepL Translation",
            "icon": "fa-globe",
            "url": "https://www.deepl.com/translator#en/de/"
          },
          "pro": {
            "name": "Issue Board",
            "icon": "clipboard-check",
            "url": "https://accelerator-fs.atlassian.net/browse/PRO-"
          },
          "cheat": {
            "name": "Cheat.sh",
            "icon": "fa-code",
            "url": "http://cheat.sh/"
          }
	      }
      }
    },
    {
      "name": "tinytacoteam/zazu-snippets",
	      "variables": {
	    "folder": "snippets"
	  }
    },
    {
      "name": "chrishelgert/zazu-fkill"
    }
  ]
}
