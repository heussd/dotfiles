Host *
  AddKeysToAgent yes
  # https://developer.apple.com/library/archive/technotes/tn2449/_index.html
  IgnoreUnknown UseKeychain
  UseKeychain yes

Host maya
  HostName maya
  User th

Host maya.local
  HostName maya.local
  User th

Host maya.fritz.box
  HostName maya.fritz.box
  User th

Host 192.168.64.5
  ForwardAgent yes
  User ubuntu

